{
	"name": "Make Chrome Better",
	"description": "My personal extension for Chrome to add shortcuts, etc. that I wanted",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": ["storage", 
		"contextMenus", 
		"activeTab",
		"scripting"],
	"background": {
		"service_worker": "worker.js"
	},	
	"icons": {
		"16": "/images/icon16.png",
		"48": "/images/icon48.png",
		"128": "/images/icon128.png"
	}
}